{
  "name": "chrome-mcp-client-rpa",
  "version": "1.0.0",
  "description": "A Chrome MCP client for RPA automation that enables browser automation, dialogue extraction, and web scraping capabilities",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node --esm src/3-exportDeepSeekDom.ts",
    "extract-dialogue": "ts-node --esm src/4-htmlDialogueExtractor.ts",
    "count-links": "ts-node --esm src/5-totalLinks.ts",
    "extract-history": "ts-node --esm src/6-historyRecordExtractor.ts",
    "ping-llm": "ts-node --esm src/llmPing.ts",
    "clear-history": "ts-node --esm src/deepseek-clear-history.ts",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "chrome",
    "mcp",
    "rpa",
    "automation",
    "web-scraping",
    "dialogue-extraction",
    "browser-automation",
    "llm",
    "deepseek"
  ],
  "author": "Chrome MCP RPA Team",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/chrome-mcp-client-rpa.git"
  },
  "bugs": {
    "url": "https://github.com/username/chrome-mcp-client-rpa/issues"
  },
  "homepage": "https://github.com/username/chrome-mcp-client-rpa#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "chrome-devtools-mcp": "^0.10.2",
    "dotenv": "^17.2.3",
    "openai": "^6.9.1",
    "selenium-webdriver": "^4.38.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/selenium-webdriver": "^4.0.24",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
